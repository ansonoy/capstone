// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgresql://postgres:password@localhost:5432/whispering_dutchman")
}

model User {
  user_id        Int      @id @default(autoincrement())
  age       Int 
  email     String   @unique
  name      String
}
